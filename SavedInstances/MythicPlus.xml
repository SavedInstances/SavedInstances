<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="SavedInstancesKeyExport" parent="UIParent" movable="true" enableMouse="true" hidden="true">
        <Size x="700" y ="450" />
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
            <BackgroundInsets>
                <AbsInset left="5" right="5" top="5" bottom="5"/>
            </BackgroundInsets>
            <TileSize>
                <AbsValue val="16"/>
            </TileSize>
            <EdgeSize>
                <AbsValue val="16"/>
            </EdgeSize>
        </Backdrop>
        <Layers>
            <Layer level="ARTWORK">
                <FontString name="SavedInstancesKeyExportText" inherits="GameFontHighlight">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset x="5" y="-5"/>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button name="SavedInstancesKeyExportButton" text="Close" inherits="GameMenuButtonTemplate">
                <Anchors>
                    <Anchor point="BOTTOM" relativeTo="SavedInstancesKeyExport" relativePoint="BOTTOM">
                        <Offset x="0" y="10"/>
                    </Anchor>
                </Anchors>
            </Button>
            <ScrollFrame name="SavedInstancesKeyExportScroll" inherits="UIPanelScrollFrameTemplate" toplevel="true">
                <Anchors>
                    <Anchor point="TOP">
                        <Offset x="0" y="-30"/>
                    </Anchor>
                    <Anchor point="BOTTOM" relativeTo="SavedInstancesKeyExportButton" relativePoint="BOTTOM">
                        <Offset x="10" y="30"/>
                    </Anchor>
                    <Anchor point="RIGHT">
                        <Offset x="-40"/>
                    </Anchor>
                </Anchors>
                <ScrollChild>
                    <EditBox name="SavedInstancesKeyExportScrollText" letters="999999" multiLine="true" autoFocus="true" enableMouse="true">
                        <Size x="630" y="380"/>
                        <FontString inherits="ChatFontNormal"/>
                    </EditBox>
                </ScrollChild>
            </ScrollFrame>
        </Frames>
    </Frame>
</Ui>